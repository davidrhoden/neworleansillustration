<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>{{ title or renderData.title or metadata.title }}</title>
  <meta name="keywords" content="{{ metadata.keywords }}" />
  <meta name="description" content="{{ metadata.description }}" />

  <!-- Facebook OpenGraph -->

  <meta property="og:type" content="website" />
  <meta property="og:locale" content="en_us" />
  <meta property="og:url" content="http://neworleansillustration.com/" />
  <meta property="og:site_name" content="New Orleans Illustration" />

  <!-- Twitter Card -->

  <meta name="twitter:card" content="summary_large_image" />
  <meta name="twitter:site" content="@neworleansillo" />
  <meta name="twitter:creator" content="@neworleansillo" />
  <meta name="twitter:title" content="New Orleans Illustration | " />
  <meta name="twitter:description" content="{{ metadata.description }}" />


  <script async src="https://identity.netlify.com/v1/netlify-identity-widget.js"></script>
  {% set css %}
    {% include "assets/css/inline.css" %}
  {% endset %}
  <style>{{ css | cssmin | safe }}</style>
  {% set js %}
    {% include "assets/js/inline.js" %}
  {% endset %}
  <script>{{ js | jsmin | safe }}</script>
</head>
